                .bind('click', function () {
                    var popUp = window.open('http://www.facebook.com/sharer.php' + '?u=' + document.URL + '&t=' + encodeURI('Зарахування на соціальний квартирний облік'),
                        'popupwindow', 'scrollbars=yes,width=800,height=400');
                    popUp.focus();
                    return false;
                });

            $('.btnTwitter')
                .attr('href', 'https://twitter.com/intent/tweet'
                    + '?original_referer=' + document.URL
                    + '&text=' + encodeURI('Зарахування на соціальний квартирний облік')
                    + '&lang=uk'
                    + '&url=' + document.URL)
                .bind('click', function () {
                    var popUp = window.open('https://twitter.com/intent/tweet'
                        + '?original_referer=' + document.URL
