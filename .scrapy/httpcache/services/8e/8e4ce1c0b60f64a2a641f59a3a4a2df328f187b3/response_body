                .attr('href', 'http://www.facebook.com/sharer.php'
                    + '?u=' + document.URL
                    + '&t=' + encodeURI('Погодження відчуження або передачі пам’яток місцевого значення та щойно виявлених об’єктів культурної спадщини їхніми власниками чи уповноваженими ними органами іншим особам у володіння, користування або управління (іпотека)'))
                .bind('click', function () {
                    var popUp = window.open('http://www.facebook.com/sharer.php' + '?u=' + document.URL + '&t=' + encodeURI('Погодження відчуження або передачі пам’яток місцевого значення та щойно виявлених об’єктів культурної спадщини їхніми власниками чи уповноваженими ними органами іншим особам у володіння, користування або управління (іпотека)'),
                        'popupwindow', 'scrollbars=yes,width=800,height=400');
                    popUp.focus();
                    return false;
                });

            $('.btnTwitter')
                .attr('href', 'https://twitter.com/intent/tweet'
                    + '?original_referer=' + document.URL
                    + '&text=' + encodeURI('Погодження відчуження або передачі пам’яток місцевого значення та щойно виявлених об’єктів культурної спадщини їхніми власниками чи уповноваженими ними органами...')
                    + '&lang=uk'
                    + '&url=' + document.URL)
                .bind('click', function () {
                    var popUp = window.open('https://twitter.com/intent/tweet'
                        + '?original_referer=' + document.URL
                        + '&text=' + encodeURI('Погодження відчуження або передачі пам’яток місцевого значення та щойно виявлених об’єктів культурної спадщини їхніми власниками чи уповноваженими ними органами...')
                        + '&lang=uk'
                        + '&url=' + document.URL,
                        'popupwindow', 'scrollbars=yes,width=800,height=400');
                    popUp.focus();
                    return false;
                });

            $('.btnGoogle')
                .attr('href', 'https://plus.google.com/share?hl=uk&url=' + encodeURI(document.URL))
                .bind('click', function () {
                    var popUp = window.open('https://plus.google.com/share?hl=uk&url=' + document.URL, 'popupwindow', 'scrollbars=yes,width=800,height=400');
                    popUp.focus();
                    return false;
                });
