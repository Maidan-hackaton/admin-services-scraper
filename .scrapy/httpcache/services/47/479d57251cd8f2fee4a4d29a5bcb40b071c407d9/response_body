        $(document).ready(function () {
            $('.btnFacebook')
                .attr('href', 'http://www.facebook.com/sharer.php'
                    + '?u=' + document.URL
                    + '&t=' + encodeURI('Визначення імені та/або прізвища дитини, яка знайдена, покинута в пологовому будинку, іншому закладі охорони здоров’я дитини'))
                .bind('click', function () {
                    var popUp = window.open('http://www.facebook.com/sharer.php' + '?u=' + document.URL + '&t=' + encodeURI('Визначення імені та/або прізвища дитини, яка знайдена, покинута в пологовому будинку, іншому закладі охорони здоров’я дитини'),
                        'popupwindow', 'scrollbars=yes,width=800,height=400');
                    popUp.focus();
                    return false;
                });

            $('.btnTwitter')
                .attr('href', 'https://twitter.com/intent/tweet'
                    + '?original_referer=' + document.URL
                    + '&text=' + encodeURI('Визначення імені та/або прізвища дитини, яка знайдена, покинута в пологовому будинку, іншому закладі охорони здоров’я дитини')
                    + '&lang=uk'
                    + '&url=' + document.URL)
                .bind('click', function () {
                    var popUp = window.open('https://twitter.com/intent/tweet'
                        + '?original_referer=' + document.URL
                        + '&text=' + encodeURI('Визначення імені та/або прізвища дитини, яка знайдена, покинута в пологовому будинку, іншому закладі охорони здоров’я дитини')
                        + '&lang=uk'
                        + '&url=' + document.URL,
                        'popupwindow', 'scrollbars=yes,width=800,height=400');
                    popUp.focus();
                    return false;
