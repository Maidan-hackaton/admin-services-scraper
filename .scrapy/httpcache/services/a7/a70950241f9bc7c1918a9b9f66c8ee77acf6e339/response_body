    <script type="text/javascript" language="javascript">
        $(document).ready(function () {
            $('.btnFacebook')
                .attr('href', 'http://www.facebook.com/sharer.php'
                    + '?u=' + document.URL
                    + '&t=' + encodeURI('Фізичні особи'))
                .bind('click', function () {
                    var popUp = window.open('http://www.facebook.com/sharer.php' + '?u=' + document.URL + '&t=' + encodeURI('Фізичні особи'),
                        'popupwindow', 'scrollbars=yes,width=800,height=400');
                    popUp.focus();
                    return false;
                });

            $('.btnTwitter')
                .attr('href', 'https://twitter.com/intent/tweet'
                    + '?original_referer=' + document.URL
                    + '&text=' + encodeURI('Фізичні особи')
                    + '&lang=uk'
                    + '&url=' + document.URL)
                .bind('click', function () {
                    var popUp = window.open('https://twitter.com/intent/tweet'
                        + '?original_referer=' + document.URL
                        + '&text=' + encodeURI('Фізичні особи')
                        + '&lang=uk'
                        + '&url=' + document.URL,
                        'popupwindow', 'scrollbars=yes,width=800,height=400');
                    popUp.focus();
                    return false;
                });

            $('.btnGoogle')
                .attr('href', 'https://plus.google.com/share?hl=uk&url=' + encodeURI(document.URL))
                .bind('click', function () {
                    var popUp = window.open('https://plus.google.com/share?hl=uk&url=' + document.URL, 'popupwindow', 'scrollbars=yes,width=800,height=400');
                    popUp.focus();
                    return false;
                });
        });
    </script>
    
